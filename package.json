{
  "name": "shvydak-dashboard",
  "version": "1.0.0",
  "description": "Shvydak's Dashboard - A modern web application for managing services",
  "type": "module",
  "main": "backend/index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run client\"",
    "backend": "cd backend && nodemon",
    "backend:build": "cd backend && tsc",
    "backend:start": "cd backend && node dist/index.js",
    "client": "cd client && npm start",
    "build": "npm run backend:build && cd client && npm run build",
    "start": "npm run backend:start",
    "install-client": "cd client && npm install",
    "install-backend": "npm install",
    "install-all": "npm run install-backend && npm run install-client",
    "lint": "cd backend && eslint src/**/*.ts",
    "lint:fix": "cd backend && eslint src/**/*.ts --fix",
    "type-check": "cd backend && tsc --noEmit",
    "seed": "cd backend && ts-node -r tsconfig-paths/register src/scripts/seed.ts"
  },
  "keywords": [
    "dashboard",
    "express",
    "react",
    "authentication"
  ],
  "author": "Shvydak",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "express-validator": "7.2.1",
    "helmet": "^7.0.0",
    "joi": "18.0.1",
    "jsonwebtoken": "9.0.2",
    "mongoose": "8.18.0",
    "morgan": "1.10.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "redis": "5.8.2",
    "typescript": "5.9.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.6",
    "@types/compression": "1.8.1",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/jsonwebtoken": "9.0.10",
    "@types/morgan": "1.9.10",
    "@types/node": "24.3.0",
    "@typescript-eslint/eslint-plugin": "8.41.0",
    "@typescript-eslint/parser": "8.41.0",
    "concurrently": "^7.6.0",
    "eslint": "9.34.0",
    "jest": "30.1.0",
    "nodemon": "2.0.22",
    "prettier": "3.6.2",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "npm@11.5.1"
}
